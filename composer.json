{
  "name": "deveodk/distributed-generators",
  "type": "library",
  "description": "Smart generators for distrubuted laravel (https://github.com/esbenp/distributed-laravel). Spin up new bundles in the matter of seconds",
  "keywords": [
    "deveodk",
    "distributed-generators"
  ],
  "homepage": "https://github.com/deveodk/distributed-generators",
  "license": "MIT",
  "authors": [
    {
      "name": "Jason Kelly",
      "email": "jk@deveo.dk",
      "homepage": "https://deveo.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "~5.6|~7.0",
    "optimus/distributed-laravel": "0.1.*"
  },
  "require-dev": {
    "phpunit/phpunit" : "~4.0||~5.0||~6.0",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "DeveoDK\\DistributedGenerators\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}